    "@fastify/compress": "^6",
    "@fastify/cookie": "9.4.0",
    "@fastify/cors": "9.0.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/multipart": "^8.3.1",
    "@fastify/static": "7.0.4",
    "bcryptjs": "^3.0.2",
    "bull": "4.16.3",
    "chalk": "^5.4.1",
    "class-transformer": "^0.5.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
