    "@types/moment": "^2.13.0",
    "@types/sharp": "^0.32.0",
    "@types/socket.io": "^3.0.2",
    "eslint": "^9.27.0",
    "globals": "^16.2.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
